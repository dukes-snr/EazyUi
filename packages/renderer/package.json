{
    "name": "@eazyui/renderer",
    "version": "1.0.0",
    "private": true,
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup && tsc --declaration --emitDeclarationOnly --outDir dist",
        "dev": "tsup --watch",
        "test": "vitest run",
        "lint": "eslint . --ext ts"
    },
    "dependencies": {
        "@eazyui/shared": "*",
        "yoga-layout": "^3.0.4",
        "konva": "^9.3.6"
    },
    "devDependencies": {
        "tsup": "^8.0.1",
        "vitest": "^1.2.0",
        "typescript": "^5.3.3"
    }
}